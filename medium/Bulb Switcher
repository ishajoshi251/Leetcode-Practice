class Solution(object):
    def bulbSwitch(self, n):
        """
        :type n: int
        :rtype: int
        """
        arr = [1]*n
        i = 1
        while i<n:
            if arr[i] == 1:
                arr[i] = 0
            else:
                arr[i] = 1      1 1 1 1 1 
            i += i
        return arr.count(1)
